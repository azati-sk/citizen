#menu{
    font-family: 'universcondensedlight';
    /*font-family: 'brandon_grotesque_regularRg';*/
    width: 400px;
    height: 100%;
    background: #333333 url(../img/asset/menu-repeat.gif) repeat-y 328px 0;
    border-right:solid 5px #bf2b2b;
    color: #fff;
    font-size: 20px;
    overflow: auto;

    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
    position: absolute;
    top:0;
    left:0;
    -webkit-font-smoothing: antialiased;
    letter-spacing: 1px;
    box-shadow: inset -9px 0px 26px rgba(0,0,0,0.3);
}

